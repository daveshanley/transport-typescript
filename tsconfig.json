{
    "compilerOptions": {
      "target": "es6",
      "declaration": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
        "dom",
        "es5",
        "es6",
        "es7",
        "es2015"
      ],
      "removeComments": false,
      "noImplicitAny": true,
      "types": [
        "jasmine"
      ]
    },
    "exclude": [
      "node_modules/*"
    ]
  }
  