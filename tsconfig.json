{
  "compilerOptions": {
    "target": "es5",
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "removeComments": false,
    "noImplicitAny": true,
    "types": [
      "jasmine"
    ],
    "outDir": "dist/",
    "rootDir": "src/"
  },
  "exclude": [
    "node_modules/*",
    "compiled/*",
    "dist/"
  ]
}
